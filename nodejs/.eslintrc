{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "no-confusing-arrow": 0,
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "camelcase": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "global-require": 0,
    "guard-for-in": 0,
    "no-restricted-syntax": 0,
    "strict": 0,
    "arrow-body-style": 0
  }
}
